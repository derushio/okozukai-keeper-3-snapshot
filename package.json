{
  "name": "okozukai-keeper-3",
  "version": "3.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "homepage": "https://example.com",
  "main": "./out/main/index.js",
  "author": "derushio",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "// test": "",
    "test:run": "vitest --reporter=html --outputFile=_testReports/index.html --run",
    "test:ui": "vitest --ui --coverage",
    "// typecheck": "",
    "typecheck": "pnpm typecheck:node && pnpm typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "// prepare": "",
    "prepare:electron": "electron-builder install-app-deps",
    "prepare:node": "pnpm rebuild",
    "prepare:browser": "echo prepare:browser",
    "// client server spa": "",
    "build": "run-p build:frontend build:backend",
    "build:frontend": "pnpm prepare:browser && vite build",
    "build:backend": "echo build:backend",
    "dev": "run-p dev:frontend dev:backend",
    "dev:frontend": "pnpm prepare:browser && vite",
    "dev:backend": "tsx --watch src/main/server.ts",
    "start": "run-p start:frontend start:backend",
    "start:frontend": "pnpm prepare:browser && vite preview --host 0.0.0.0 --port 5173 --strictPort true",
    "start:backend": "tsx src/main/server.ts",
    "// drizzle": "",
    "drizzle:init": "tsx ./src/main/infrastructures/db/okozukai_keeper_3/index.ts",
    "drizzle:generate": "tsx ./node_modules/drizzle-kit/bin.cjs generate:pg",
    "drizzle:drop": "tsx ./node_modules/drizzle-kit/bin.cjs drop",
    "drizzle:migrate": "tsx ./src/main/infrastructures/db/okozukai_keeper_3/index.ts && tsx ./src/main/infrastructures/db/okozukai_keeper_3/utils/migrate.ts",
    "drizzle:seed": "tsx ./src/main/infrastructures/db/okozukai_keeper_3/index.ts && tsx ./src/main/infrastructures/db/okozukai_keeper_3/utils/migrate.ts && tsx ./src/main/infrastructures/db/okozukai_keeper_3/seed/index.ts",
    "drizzle:studio": "tsx ./node_modules/drizzle-kit/bin.cjs studio",
    "drizzle:pull": "tsx ./node_modules/drizzle-kit/bin.cjs introspect:pg",
    "drizzle:migration_from_old_db": "tsx ./src/main/infrastructures/db/okozukai_keeper_3/seed/migrationFromOldDb/index.ts",
    "// drizzle:old": "",
    "drizzle:old:studio": "tsx ./node_modules/drizzle-kit/bin.cjs studio --config drizzle.old.config.ts",
    "drizzle:old:pull": "tsx ./node_modules/drizzle-kit/bin.cjs introspect:pg --config drizzle.old.config.ts",
    "// batch": "",
    "batch:executeOkozukaiBoardSchedules": "tsx ./src/main/batches/executeOkozukaiBoardSchedules.ts",
    "// env": "",
    "env:build": "cp .env .env.stash && cp .env.build .env",
    "env:restore": "cp .env.stash .env",
    "postinstall": ""
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@hebilicious/vue-query-nuxt": "^0.2.1",
    "@pinia/nuxt": "^0.5.1",
    "@quasar/extras": "^1.16.9",
    "@quasar/vite-plugin": "^1.6.0",
    "@tanstack/vue-query": "^5.12.2",
    "@trpc/client": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "@vitejs/plugin-vue": "^5.0.1",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "better-sqlite3": "~9.2.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.1",
    "drizzle-zod": "^0.5.1",
    "express": "^4.18.2",
    "formdata-node": "^6.0.3",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.9.0",
    "nuxt-quasar-ui": "^2.0.6",
    "p-queue": "^7.4.1",
    "pg": "^8.11.3",
    "pinia": "^2.1.7",
    "quasar": "^2.14.1",
    "superjson": "^1.13.3",
    "tsx": "^4.7.0",
    "undici": "^6.0.1",
    "uuid": "^9.0.1",
    "vite": "^5.0.10",
    "vue": "^3.3.10",
    "vue-router": "^4.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.4",
    "@nuxt/devtools": "^1.0.4",
    "@quasar/app-webpack": "^3.12.1",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.4.0",
    "@rushstack/eslint-patch": "^1.6.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@typescript-eslint/typescript-estree": "^5.62.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.7",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.3",
    "drizzle-kit": "^0.20.6",
    "electron-vite": "^1.0.29",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-vue": "^9.19.2",
    "happy-dom": "^12.10.3",
    "node-gyp": "^10.0.1",
    "prettier": "^2.8.8",
    "sass": "^1.69.5",
    "typescript": "^5.3.3",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.25"
  }
}
